<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="util.johndon.cmcc.com.news1">

    <application
        android:allowBackup="true"
        android:icon="@drawable/app_lunache_icon_one"
        android:label="@string/news_app_name"
        android:roundIcon="@drawable/app_lunache_icon_one"
        android:supportsRtl="true"
        tools:replace="android:label"
        android:theme="@style/AppTheme">
        <activity android:name="util.johndon.cmcc.com.news1.ChooseTypeActivity"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="util.johndon.cmcc.com.news1.MainActivity"
            android:screenOrientation="portrait"/>
    </application>
    <uses-permission android:name="android.permission.INTERNET"/>
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"/>

</manifest>